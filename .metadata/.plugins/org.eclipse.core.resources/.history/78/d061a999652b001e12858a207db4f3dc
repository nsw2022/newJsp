<body>
	<h1>3,6,9에 짝이 들어가요</h1>	
      <%
    for (int i = 1; i <= 100; i++) {
        String test = Integer.toString(i);
        int is369 = 0;

        if (test.contains("3") || test.contains("6") || test.contains("9")) {
            is369++;
        }

        if (is369==1) {
            out.print("짝"+"<br>");
        }else if(is369==2){
        	out.print("짝짝"+"<br>");
        	System.out.println(is369);
        }
        else {
            out.print(i + " "+"<br>");
        }
    }
    %>
