package basic_p;

public class CalcMain {
	public static void main(String[] args) {
		int aa = 5;
		aa++;
		System.out.println(aa);
		
		++aa;
		System.out.println(aa+"여기서 7");
		System.out.println(aa++);
		System.out.println(++aa);
		
		int a=5, b=6, c=10, d;
		d= a++ + ++c - b-- * --a + a++;
		// 5   +  11 - 6  *    5 + 5
		// d=
		
		// a=6 b=5 c=11 d=-9
		System.out.println(a+"a");
		System.out.println(b+"b");
		System.out.println(c+"c");
		System.out.println(d+"d");
		
		
		
	}


}
