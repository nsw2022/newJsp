package basic_p;

public class CalcMain {
	public static void main(String[] args) {
		int aa = 5;
		aa++;
		System.out.println(aa);
		
		++aa;
		System.out.println(aa+"여기서 7");
		System.out.println(aa++);
		System.out.println(++aa);
		
		int a=5, b=6, c=10,d;
		d= a++ + ++c - b-- * --a + a++;
		// 5   +  7  - 6   *   4 + 5
		// d=
		System.out.println(d);
		
		
		
	}


}
