<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>369</title>
</head>
<body>
	<h1>3,6,9에 짝이 들어가요</h1>	
    <%
    for (int i = 0; i < 100; i++) {
        String test = Integer.toString(i);
        int count369 = 0;

        for (int j = 0; j < test.length(); j++) {
            char digit = test.charAt(j);
            if (digit == '3' || digit == '6' || digit == '9') {
                count369++;
            }
        }

        if (count369 == 2) {
            out.print("짝짝 ");
        } else if (count369 > 0) {
            out.print("짝 ");
        } else {
            out.print(i + " ");
        }
    }
    %>
</body>
</html>