	<h1>3,6,9에 짝이 들어가요</h1>
		<%
		int num1 = 10, num2 = 20;
		int sum;
		sum = num1 + num2;
		out.println("합계: " + sum); // out 내장객체
		out.println("<br>");
		
		// 1 부터 10까지
		for (int i = 1; i <= 100; i++) {
		    String forI = Integer.toString(i);
		    int is369 = 0;
		
		    for (int j = 0; j < forI.length(); j++) {
		        char digit = forI.charAt(j);
		        if (digit == '3' || digit == '6' || digit == '9') {
		            is369 = 1;
		            break;
		        }
		        if( digit == '33' || digit == '66' || digit == '99'){
		        	is369 = 2;
		        	break;
		        }
		    }
		
		    if (is369==1) {
		        out.println("짝");
		    }else if(is369==2){
		    	out.println("짝짝");
		    }else {
		        out.println(i);
		    }
		}
		
		

