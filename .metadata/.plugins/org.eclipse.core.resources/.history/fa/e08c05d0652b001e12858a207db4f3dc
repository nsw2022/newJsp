<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>369</title>
</head>
<body>
	<h1>3,6,9에 짝이 들어가요</h1>	
     <%
    for (int i = 0; i < 100; i++) {
        String test = Integer.toString(i);
        int count369 = 0;
        StringBuilder result = new StringBuilder();

        for (int j = 0; j < test.length(); j++) {
            char digit = test.charAt(j);
            if (digit == '3' || digit == '6' || digit == '9') {
                count369++;
            }
        }

        for (int k = 0; k < count369; k++) {
            result.append("짝");
        }

        if (result.length() == 0) {
            out.print(i + "<br>");
        } else {
            out.print(result + "<br> ");
        }
    }
    %>
</body>
</html>